{
    "name": "makaveli/laravel-core",
    "description": "Core for Laravel",
    "keywords": ["laravel", "framework"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Udovenko",
            "email": "mikhail.udovenko.02@mail.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.10|^11.0|^12.0",
        "makaveli/laravel-converter": "1.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [],
            "providers": [
                "Core\\Providers\\CoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
